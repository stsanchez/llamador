#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Workspace/llamador_node/hospital-system/node_modules/mime/node_modules:/mnt/d/Workspace/llamador_node/hospital-system/node_modules:/mnt/d/Workspace/llamador_node/node_modules:/mnt/d/Workspace/node_modules:/mnt/d/node_modules:/mnt/d/Workspace/llamador_node/hospital-system/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Workspace/llamador_node/hospital-system/node_modules/mime/node_modules:/mnt/d/Workspace/llamador_node/hospital-system/node_modules:/mnt/d/Workspace/llamador_node/node_modules:/mnt/d/Workspace/node_modules:/mnt/d/node_modules:/mnt/d/Workspace/llamador_node/hospital-system/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else
  exec node  "$basedir/../mime/cli.js" "$@"
fi
